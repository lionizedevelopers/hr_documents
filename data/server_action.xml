<?xml version="1.0"?>
<odoo>
    <!-- Server Action to Upload Multiple images -->
    <record id="action_server_upload_multi_images" model="ir.actions.server">
        <field name="name">Bulk Upload</field>
        <field name="type">ir.actions.server</field>
        <field name="model_id" ref="hr_documents.model_hr_employee" />
        <field name="binding_model_id" ref="hr_documents.model_hr_employee" />
        <field name="state">code</field>
        <field name="code">
            action = records.bulk_upload()
        </field>
    </record>
</odoo>